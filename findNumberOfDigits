class Solution {
    public int findNumbers(int[] nums) {
        int noOfDigitsCount = 0;
        for(int i = 0;i<nums.length;i++)
        {
            String number = String.valueOf(nums[i]);
            if(number.length()%2 == 0)
                {
                    noOfDigitsCount++;
                }
        }
        return noOfDigitsCount;
        }
}
